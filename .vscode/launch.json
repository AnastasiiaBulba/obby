{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Launch Live Server",
      "type": "node",
      "request": "launch",
      "program": "${workspaceFolder}/index.html",
      "serverReadyAction": {
        "pattern": "Local: http://localhost:([0-9]+)",
        "uriFormat": "http://localhost:%s",
        "action": "openExternally"
      }
    }
  ]
}
